{
  "manifest_version": 3,
  "name": "银行合同数据助手",
  "version": "1.1.0",
  "description": "解析并快速复制银行合同数据文件中的字段",
  "permissions": [
    "storage",
    "sidePanel"
  ],
  "action": {
    "default_title": "打开数据助手侧边栏"
  },
  "side_panel": {
    "default_path": "popup.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}